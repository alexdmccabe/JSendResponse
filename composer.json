{
    "name": "artyuum/symfony-jsend-response",
    "description": "JSendResponse component for HttpFoundation based applications (Symfony, Silex, Laravel, Drupal, etc.). It follows the JSend specification, allowing you to give consistent JSON responses to your users.",
    "keywords": [
        "symfony",
        "jsend",
        "response",
        "silex",
        "drupal",
        "api",
        "json"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Dmitry Kosenkov",
            "email": "dk-junker@ya.ru"
        },
        {
            "name": "Artyum Petrov",
            "email": "artyum@protonmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Junker\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.2 || ^8.0",
        "ext-json": "*",
        "symfony/http-foundation": "^4.0 || ^5.0 || ^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    }
}
